.box {
  width: 1730px;
  height: 1282px;
}

.box .my-journey {
  width: 1730px;
  height: 1282px;
  top: 0;
  left: 0;
}

.box .overlap {
  position: relative;
  height: 1282px;
}

.box .group {
  position: absolute;
  width: 1730px;
  height: 1282px;
  top: 0;
  left: 0;
}

.box .overlap-group {
  position: relative;
  width: 1280px;
  height: 1282px;
  left: 243px;
}

.box .div {
  position: absolute;
  width: 1280px;
  height: 1282px;
  top: 0;
  left: 0;
}

.box .flowers {
  width: 282px;
