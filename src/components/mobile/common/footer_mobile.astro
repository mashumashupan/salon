---
import logo from "~/assets/logomark.png";
import insta from "~/assets/footer/icon_insta.svg";
import twitter from "~/assets/footer/icon_x.svg";
import note from "~/assets/footer/icon_note.svg";
import menu from "~/assets/icon_line.svg";
---

<div class="box">
  <footer class="footer">
    <p class="element-all-rights">© 2024 Ureshino Salon.&nbsp;&nbsp;All Rights Reserved</p>
    <div class="logo">
      <img class="img" src={"img/logo.png"} />
      <div class="salon">
        <div class="text-wrapper">うれしのサロン</div>
        <div class="div">運営会社：Empression Co., Ltd.</div>
      </div>
    </div>
    <div class="privacy">
      <div class="text-wrapper-2">Privacy Policy</div>
      <div class="text-wrapper-2">特定商取引法に基づく表記</div>
    </div>
    <div class="social">
      <a
        href="10代で起業した美女・嬉野ゆみ、恋人に“伸び代”求める理由とは?:https://mdpr.jp/news/detail/3568425"
        target="_blank"
        rel="noopener noreferrer"
        ><img class="twitter-x" src="img/twitter-x-1.svg"
      /></a>
      <a
        href="10代で起業した美女・嬉野ゆみ、恋人に“伸び代”求める理由とは?:https://mdpr.jp/news/detail/3568425"
        target="_blank"
        rel="noopener noreferrer"
        ><img class="instagram" src="img/icons8-instagram-1.svg"
      /></a>
      <a
        href="10代で起業した美女・嬉野ゆみ、恋人に“伸び代”求める理由とは?:https://mdpr.jp/news/detail/3568425"
        target="_blank"
        rel="noopener noreferrer"
        ><img class="note-long" src="img/note-long.svg"
      /></a>
      <img class="linepink" src="img/linepink-1.svg" />
    </div>
  </footer>
</div>

