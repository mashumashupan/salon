.box {
  position: relative;
  width: 321px;
  height: 316px;
}

.box .footer {
  width: 323px;
  height: 316px;
  top: 0;
  left: 0;
  background-color: transparent;
}

.box .element-all-rights {
  position: absolute;
  top: 296px;
  left: 7px;
  font-family: var(--montserrat-font-family);
  font-weight: var(--montserrat-font-weight);
  color: var(--variable-collection-white);
  font-size: var(--montserrat-font-size);
  letter-spacing: var(--montserrat-letter-spacing);
  line-height: var(--montserrat-line-height);
  white-space: nowrap;
  font-style: var(--montserrat-font-style);
}

.box .logo {
  display: inline-flex;
  flex-direction: column;
  align-items: center;
  position: absolute;
  top: 0;
  left: 66px;
}

.box .img {
  position: relative;
  width: 123px;
  height: 95px;
  object-fit: cover;
}

.box .salon {
  display: inline-flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  position: relative;
  flex: 0 0 auto;
}

.box .text-wrapper {
  position: relative;
  width: fit-content;
  margin-top: -1px;
  font-family: var(--small-title-j-font-family);
  font-weight: var(--small-title-j-font-weight);
  color: var(--variable-collection-white);
  font-size: var(--small-title-j-font-size);
  letter-spacing: var(--small-title-j-letter-spacing);
  line-height: var(--small-title-j-line-height);
  font-style: var(--small-title-j-font-style);
}

.box .div {
  position: relative;
  width: fit-content;
  margin-top: -2px;
  font-family: var(--small-text-j-font-family);
  font-weight: var(--small-text-j-font-weight);
  color: var(--variable-collection-white);
  font-size: var(--small-text-j-font-size);
