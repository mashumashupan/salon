---
import { Image } from "astro:assets";

import layer1 from "~/assets/layer/layer_contents1.png";
import layer2 from "~/assets/layer/layer_contents2.png";

import contents1_live from "~/assets/contents/contents1_live.png";
import contents2_meetup from "~/assets/contents/contents2_meetup.png";
import contents3_korea from "~/assets/contents/contents3_korea.png";
import contents4_column from "~/assets/contents/contents4_column.png";
import contents5_slack from "~/assets/contents/contents5_slack.png";
import contents6_bukatu from "~/assets/contents/contents6_bukatu.png";
import subtract from "~/assets/contents/Subtract.svg";
import Line1 from "~/assets/contents/Line1.svg";
import Line2 from "~/assets/contents/Line2.svg";
import watercolor from "~/assets/watercolor1_1.png";

import meetup1 from "~/assets/contents/meetup1.png";
import meetup2 from "~/assets/aboutimage.png";
import meetup3 from "~/assets/contents/meetup3.png";
import meetup4 from "~/assets/contents/meetup4.png";
import meetup5 from "~/assets/benefit/benefit2.png";
import meetup6 from "~/assets/contents/meetup6.png";
import meetup7 from "~/assets/communityimage.png";
import meetup8 from "~/assets/contents/meetup8.png";
import meetup9 from "~/assets/contents/meetup9.png";

import button from "~/assets/button.svg";
---

<div class="box">
  <div class="contents">
    <div class="overlap">
      <Image class="leaf-shadow" src={layer1} alt="" />
      <Image class="img" src={layer2} alt="" />
      <div class="title">
        <div class="overlap-group">
          <div class="div">
            <div class="header-mark"></div>
            <div class="text-wrapper">特典コンテンツ</div>
          </div>
          <div class="member-only">
            <div class="text-wrapper-2">メンバー限定</div>
            <Image class="line" src={Line1} alt="" />
            <Image class="line-2" src={Line2} alt="" />
          </div>
        </div>
      </div>
      <div class="dots">
        <div class="ellipse"></div>
        <div class="ellipse-2"></div>
        <div class="ellipse-3"></div>
      </div>
      <div class="label">
        <div class="overlap-2">
          <div class="group">
            <div class="watercolor-wrapper"><Image class="watercolor" src={watercolor} alt="" /></div>
          </div>
          <div class="text-wrapper-3">今が入会の</div>
          <div class="text-wrapper-4">チャンス！</div>
        </div>
      </div>
      <div class="default-button">
        <div class="overlap-3">
          <div class="frame">
            <div class="text-wrapper-5">今すぐ入会</div>
            <Image class="arrow-forward-ios" src={button} alt="" />
          </div>
          <img class="intersect" src="img/intersect.svg" />
        </div>
      </div>
      <div class="gallery">
        <div class="overlap-4">
          <div class="pic">
            <div class="frame-2">
              <div class="group-1">
                <Image class="meetup" src={meetup1} alt="" />
                <div class="div-wrapper"><div class="text-wrapper-6">Korea 2020</div></div>
              </div>
              <div class="group-2">
                <Image class="meetup" src={meetup2} alt="" />
                <div class="div-wrapper"><div class="text-wrapper-6">Korea 2020</div></div>
              </div>
              <div class="group-3">
                <Image class="meetup" src={meetup3} alt="" />
                <div class="div-wrapper"><div class="text-wrapper-6">Korea 2020</div></div>
              </div>
            </div>
            <div class="frame-2">
              <div class="group-4">
                <Image class="meetup" src={meetup4} alt="" />
                <div class="div-wrapper"><div class="text-wrapper-6">Malaysia 2020</div></div>
              </div>
              <div class="group-5">
                <Image class="meetup" src={meetup5} alt="" />
                <div class="div-wrapper"><div class="text-wrapper-6">Malaysia 2020</div></div>
              </div>
              <div class="group-6">
                <Image class="meetup" src={meetup6} alt="" />
                <div class="div-wrapper"><div class="text-wrapper-6">Korea 2020</div></div>
              </div>
            </div>
            <div class="frame-2">
              <div class="group-7">
                <Image class="meetup" src={meetup7} alt="" />
                <div class="div-wrapper"><div class="text-wrapper-6">Korea 2020</div></div>
              </div>
              <div class="group-8">
                <Image class="meetup" src={meetup8} alt="" />
                <div class="div-wrapper"><div class="text-wrapper-6">Korea 2020</div></div>
              </div>
              <div class="group-9">
                <Image class="meetup" src={meetup9} alt="" />
                <div class="div-wrapper"><div class="text-wrapper-6">Tokyo 2020</div></div>
              </div>
            </div>
          </div>
          <div class="overlap-wrapper">
            <div class="overlap-5">
              <div class="schedule">
                <div class="overlap-group-2">
                  <div class="rectangle"></div>
                  <div class="frame-3">
                    <div class="frame-4">
                      <div class="frame-5">
                        <div class="text-wrapper-7">4月27日 (土)</div>
                        <div class="text-wrapper-8">19:00 - 21:30</div>
                      </div>
                      <div class="text-wrapper-9">韓国</div>
                    </div>
                    <div class="frame-4">
                      <div class="frame-5">
                        <div class="text-wrapper-7">5月1日 (水)</div>
                        <div class="text-wrapper-8">19:00 - 21:30</div>
                      </div>
                      <div class="text-wrapper-9">韓国</div>
                    </div>
                    <div class="frame-4">
                      <div class="frame-5">
                        <div class="text-wrapper-7">5月12日 (日)</div>
                        <div class="text-wrapper-8">12:00 - 14:30</div>
                      </div>
                      <div class="text-wrapper-9">大阪</div>
                    </div>
                    <div class="frame-4">
                      <div class="frame-5">
                        <div class="text-wrapper-7">5月14日 (火)</div>
                        <div class="text-wrapper-8">19:00 - 21:30</div>
                      </div>
                      <div class="text-wrapper-9">大阪</div>
                    </div>
                    <div class="frame-4">
                      <div class="frame-5">
                        <div class="text-wrapper-7">5月15日 (水)</div>
                        <div class="text-wrapper-8">19:00 - 21:30</div>
                      </div>
                      <div class="text-wrapper-9">名古屋</div>
                    </div>
                    <div class="frame-4">
                      <div class="frame-5">
                        <div class="text-wrapper-7">5月17日 (金)</div>
                        <div class="text-wrapper-8">19:00 - 21:30</div>
                      </div>
                      <div class="text-wrapper-9">東京</div>
                    </div>
                    <div class="frame-4">
                      <div class="frame-5">
                        <div class="text-wrapper-7">5月18日 (土)</div>
                        <div class="text-wrapper-8">12:00 - 14:30</div>
                      </div>
                      <div class="text-wrapper-9">東京</div>
                    </div>
                    <div class="frame-4">
                      <div class="frame-5">
                        <div class="text-wrapper-7">5月19日 (日)</div>
                        <div class="text-wrapper-8">12:00 - 14:30</div>
                      </div>
                      <div class="text-wrapper-9">東京</div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="text-wrapper-10">Meetup</div>
            </div>
          </div>
        </div>
      </div>
